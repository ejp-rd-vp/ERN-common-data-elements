    sio:SIO_000300 "1995-01-01"^^xsd:date;
    sio:SIO_000628 :birthdateattribute_ .

:birthdateattribute_ a SIO:000614, obo:NCIT_C68615 ;
    rdfs:label "Birth Date"^^xsd:string .

:sexrole_  a obo:OBI_0000093, a sio:SIO_000016;
    rdfs:label "Patient For Gender Assessment"^^xsd:string;
    sio:SIO_000356 :sexprocess_ .

:sexprocess_ a sio:SIO_000006 ;
    rdfs:label "sex measuring process"^^xsd:string;
    sio:SIO_000229 :sexoutput_ .

:sexoutput_ a sio:SIO_000015;
