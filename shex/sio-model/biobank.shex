PREFIX : <http://purl.org/ejp-rd/cde/v020/shex/>
PREFIX obo: <http://purl.obolibrary.org/obo/> 
PREFIX sio: <http://semanticscience.org/resource/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dc: <http://purl.org/dc/elements/1.1/>

:personShape IRI { 
  a [sio:SIO_000498];
  sio:SIO_000228 @:roleShape
}

:roleShape IRI {
  a [obo:OBI_0000093];
  sio:SIO_000356 @:sampleCollectionShape
}

:sampleCollectionShape IRI {
  a [obo:OBI_0000659];
  sio:SIO_000229 @:specimenShape
}

:specimenShape IRI {
  a [obo:OBI_0100051] 
}

:biobankShape IRI {
  a [obo:OMIABIS_0000010] ;
  dc:title xsd:string;
  sio:SIO_000228 @:biobankRoleShape
}

:biobankRoleShape IRI {
  a [obo:OBI_0000947];
  sio:SIO_000356 @:sampleStorageShape
}

:sampleStorageShape IRI {
  a [obo:ICO_0000060];
  sio:SIO_000230 @:specimenShape
}
